<Page x:Class="HIITT.Paginas.EjerciciosPag"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:HIITT.Paginas"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="EjerciciosPag">
    <ScrollViewer>
        <StackPanel>
            <StackPanel x:Name="MainStackPanel" Grid.Row="0"/>
            <!-- No tiene evento -->
            <Button Content="Agregar Ejercicio" Style="{StaticResource EstiloBotonesRutinas}" Width="200" Grid.Row="1" VerticalAlignment="Center" HorizontalAlignment="Center" Margin="0,30,20,0"/>
        </StackPanel>
    </ScrollViewer>
</Page>
